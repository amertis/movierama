{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "models",
      "models",
      "daos",
      "db",
      "http",
      "middleware",
      "services"
    ],
    "exclude": ["controllers"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName"      : "DocStrap",
    "footer"          : "",
    "copyright"       : "DocStrap Copyright Â© 2012-2013 The contributors to the JSDoc3 and DocStrap projects.",
    "navType"         : "vertical",
    "theme"           : "cerulean",
    "linenums"        : true,
    "collapseSymbols" : false,
    "inverseNav"      : true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/js",
    "recurse": true,
    "readme": "./README.md",
    "template": "./node_modules/docdash",
    "verbose": "true"
  }
}